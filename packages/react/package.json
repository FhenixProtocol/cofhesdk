    "react-icons": "^5.5.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^4.0.0",
    "zustand": "^5.0.1"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "viem": "^2.38.6"
