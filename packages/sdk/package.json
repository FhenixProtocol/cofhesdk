{
  "name": "@cofhesdk/sdk",
  "version": "0.0.0",
  "type": "module",
  "description": "CoFHE SDK - Complete SDK for FHE operations on Fhenix",
  "main": "./dist/node.js",
  "module": "./dist/node.mjs",
  "types": "./dist/node.d.ts",
  "browser": "./dist/web.mjs",
  "exports": {
    ".": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.js",
      "types": "./dist/node.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters.mjs",
      "require": "./dist/adapters.js",
      "types": "./dist/adapters.d.ts"
    },
    "./chains": {
      "import": "./dist/chains.mjs",
      "require": "./dist/chains.js",
      "types": "./dist/chains.d.ts"
    },
    "./permits": {
      "import": "./dist/permits.mjs",
      "require": "./dist/permits.js",
      "types": "./dist/permits.d.ts"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.js",
      "types": "./dist/node.d.ts"
    },
    "./web": {
      "import": "./dist/web.mjs",
      "require": "./dist/web.js",
      "types": "./dist/web.d.ts"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "immer": "^10.1.1",
    "zustand": "^5.0.1",
    "zod": "^3.22.0",
    "viem": "^2.0.0",
    "node-tfhe": "0.11.1",
    "idb-keyval": "^6.2.1",
    "tfhe": "0.11.1",
    "tweetnacl": "^1.0.3"
  },
  "peerDependencies": {
    "viem": "^2.0.0",
    "@wagmi/core": "^2.0.0",
    "ethers": "^5.0.0 || ^6.0.0",
    "hardhat": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@wagmi/core": {
      "optional": true
    },
    "ethers": {
      "optional": true
    },
    "hardhat": {
      "optional": true
    },
    "@nomicfoundation/hardhat-ethers": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cofhesdk/eslint-config": "workspace:*",
    "@cofhesdk/tsconfig": "workspace:*",
    "@types/node": "^20.0.0",
    "@vitest/browser": "1.6.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "^8.57.0",
    "playwright": "^1.55.0",
    "tsup": "^8.0.2",
    "typescript": "5.5.4",
    "vitest": "1.6.0",
    "ethers5": "npm:ethers@^5.7.2",
    "ethers6": "npm:ethers@^6.13.0",
    "hardhat": "^2.19.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
